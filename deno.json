    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@types/node": "npm:@types/node@^22.13.8",
    "yaml": "npm:yaml@^2.7.0"
